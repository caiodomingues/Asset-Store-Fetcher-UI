<table class="text-center">
    <tr class="bold">
        <% json.bigData.declinedPackages.forEach(package => { %>
        <th>
            <%= package.name %>
        </th>
        <% }) %>
    </tr>
    <tr>
        <% json.bigData.declinedPackages.forEach(package => { %>
        <td>
            <%= json.currency %>
            <%= package.price %>
        </td>
        <% }) %>
    </tr>
    <tr>
        <% json.bigData.declinedPackages.forEach(package => { %>
        <td>
            <a target="_blank" href=<%- package.short_url %>>
                Open at Asset Store
            </a>
        </td>
        <% }) %>
    </tr>
</table>